<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="DarkBaseColor">#4E4349</Color>
    <SolidColorBrush x:Key="DarkBaseBrush" Color="{StaticResource DarkBaseColor}"/>

    <Color x:Key="LightBaseColor">#FFB380</Color>
    <SolidColorBrush x:Key="LightBaseBrush" Color="{StaticResource LightBaseColor}"/>

    <Color x:Key="AccentColor">#866574</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    
    <Color x:Key="GrayColor">Gray</Color>
    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource GrayColor}"/>

    <Color x:Key="WhiteColor">White</Color>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}"/>

    <Color x:Key="BlackColor">Black</Color>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource BlackColor}"/>

</ResourceDictionary>